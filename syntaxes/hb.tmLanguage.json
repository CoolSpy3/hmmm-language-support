{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "HMMM Binary",
	"patterns": [
		{
			"include": "#binary"
		}
	],
	"repository": {
		"binary": {
			"patterns": [
				{
					"match": "\\b[01 ]+\\b",
					"name": "constant.numeric.binary.hb"
				}
			]
		}
	},
	"scopeName": "source.hb"
}
