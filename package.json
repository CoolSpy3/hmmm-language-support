{
  "name": "hmmm-language-support",
  "displayName": "HMMM Language Support",
  "description": "Adds HMMM language support to VSCode",
  "version": "1.0.1",
  "publisher": "CoolSpy3",
  "main": "./client/out/extension.js",
  "categories": [
    "Programming Languages"
  ],
  "keywords": [
    "HMMM"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/CoolSpy3/hmmm-language-support"
  },
  "engines": {
    "vscode": "^1.85.0"
  },
  "contributes": {
    "languages": [{
      "id": "hmmm",
      "aliases": ["HMMM", "hmmm"],
      "extensions": [".hmmm"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "hmmm",
      "scopeName": "source.hmmm",
      "path": "./syntaxes/hmmm.tmLanguage.json"
    }]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -b",
    "watch": "tsc -b -w",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint ./client/src ./server/src --ext .ts,.tsx",
    "postinstall": "cd client && npm install && cd ../server && npm install",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.85.0",
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "eslint": "^8.54.0",
    "typescript": "^5.3.2",
    "@vscode/test-cli": "^0.0.4",
    "@vscode/test-electron": "^2.3.8"
  }
}
