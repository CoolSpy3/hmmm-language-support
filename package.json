{
  "name": "hmmm-language-support",
  "displayName": "HMMM Language Support",
  "description": "Adds HMMM language support to VSCode",
  "version": "1.0.0",
  "publisher": "CoolSpy3",
  "engines": {
    "vscode": "^1.82.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "keywords": [
    "HMMM"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/CoolSpy3/hmmm-language-support"
  },
  "contributes": {
    "languages": [{
      "id": "hmmm",
      "aliases": ["HMMM", "hmmm"],
      "extensions": [".hmmm"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "hmmm",
      "scopeName": "source.hmmm",
      "path": "./syntaxes/hmmm.tmLanguage.json"
    }]
  }
}
